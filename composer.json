{
    "name": "spryker-demo/service-product",
    "type": "library",
    "description": "ServiceProduct module",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "spryker/kernel": "^3.73.0",
        "spryker/merchant-sales-order": "^1.4.0",
        "spryker/product": "^6.37.0",
        "spryker/sales": "^11.40.5",
        "spryker/shipment-extension": "^1.2.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "^0.17.18",
        "spryker/state-machine": "^2.18.0"
    },
    "autoload": {
        "psr-4": {
            "SprykerDemo\\": "src/SprykerDemo/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerDemoTest\\": "tests/SprykerDemoTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "suggest": {
        "spryker/state-machine": "^2.18.0"
    }
}
